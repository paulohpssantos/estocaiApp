{
  "expo": {
    "name": "EstocaFacilApp",
    "slug": "EstocaiApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "estocaiapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.paulohps.EstocaiApp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.paulohps.EstocaiApp"
    },
    "web": {
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-router",
        {
          "origin": "https://estocaiapp.expo.app"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.paulohps.EstocaiApp",
          "enableGooglePay": true,
          "publishableKey": "pk_test_51KzYOkLh5D9uQTs1Yy8J1kT3vR7Di0p3Y6j6k1bXKJ4v3hX4Z5gW8Jt1eF2gX9z5Yz"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "origin": "https://estocaiapp.expo.app"
      },
      "eas": {
        "projectId": "dac97d43-6d32-4d1f-a01d-1b5ec8881fd8"
      }
    }
  },
  "devDependencies": {
    "@react-native-community/cli": "latest",
  }
}
